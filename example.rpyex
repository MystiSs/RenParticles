rparticles as rparticles_test onlayer master zorder 1:
    sprite expr Solid("#960000", xysize=(12, 12)); test_particle
    lifetime range random (1.5, 3.0)
    redraw 0.0
    on update:
        emitter custom renparticles.EmitterRandom oneshot:
            amount 250
            area (0, 0, config.screen_width, config.screen_height)
        custom renparticles.RepulsorUpdate
        custom renparticles.LifetimeDeltaDecreaser
    on event:
        custom renparticles.RepulsorEvent
    on particle dead:
        emitter custom renparticles.EmitterRandom:
            amount 1
            area (0, 0, config.screen_width, config.screen_height)

