            amount 100
        preset repulsor:
            strength 32.0

        preset auto_expire

        on update:
            oscillate:
                amplitudes [0.0, 0.0]
                amplitudes_range [125.0, 400.0]

            move:
                velocity [0.0, 0.0]
                velocity_range [200.0, 100.0]

                acceleration [150.0, 400.0]

        on particle dead:
